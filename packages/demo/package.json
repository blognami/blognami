{
  "private": true,
  "type": "module",
  "name": "demo",
  "version": "0.32.0",
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "start": "npm run start:main",
    "start:main": "IS_LOCAL_PINSTRIPE=true pinstripe start-server",
    "start:docs": "IS_LOCAL_PINSTRIPE=true pinstripe start-server --app docs",
    "test:models": "npm run test:models:$(echo ${DATABASE_ADAPTER:-sqlite})",
    "test:models:mysql": "DATABASE_ADAPTER=mysql SKIP_FIXTURES=true NODE_OPTIONS=--experimental-vm-modules jest --runInBand test/models",
    "test:models:sqlite": "DATABASE_ADAPTER=sqlite SKIP_FIXTURES=true NODE_OPTIONS=--experimental-vm-modules jest --runInBand test/models",
    "test:e2e": "npm run test:e2e:$(echo ${DATABASE_ADAPTER:-sqlite})",
    "test:e2e:mysql": "NODE_ENV=test DATABASE_ADAPTER=mysql start-server-and-test 'pinstripe reset-database-and-dump-sql && npm run start' http://127.0.0.1:3000 'npm run cypress:run'",
    "test:e2e:sqlite": "NODE_ENV=test DATABASE_ADAPTER=sqlite start-server-and-test 'pinstripe reset-database-and-dump-sql && npm run start' http://127.0.0.1:3000 'npm run cypress:run'",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "blognami": "^0.31.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "cypress": "^13.10.0",
    "jest": "^28.0.1",
    "start-server-and-test": "^1.15.2"
  }
}
