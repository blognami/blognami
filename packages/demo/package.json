{
  "private": true,
  "type": "module",
  "name": "demo",
  "version": "0.22.0",
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "start": "IS_LOCAL_SINTRA=true sintra start-server",
    "test:models": "yarn test:models:$(echo ${DATABASE_ADAPTER:-sqlite})",
    "test:models:mysql": "DATABASE_ADAPTER=mysql SKIP_FIXTURES=true NODE_OPTIONS=--experimental-vm-modules jest --runInBand test/models",
    "test:models:sqlite": "DATABASE_ADAPTER=sqlite SKIP_FIXTURES=true NODE_OPTIONS=--experimental-vm-modules jest --runInBand test/models",
    "test:e2e": "yarn test:e2e:$(echo ${DATABASE_ADAPTER:-sqlite})",
    "test:e2e:mysql": "NODE_ENV=test DATABASE_ADAPTER=mysql start-server-and-test 'sintra reset-database && yarn start' http://127.0.0.1:3000 'yarn cypress:run'",
    "test:e2e:sqlite": "NODE_ENV=test DATABASE_ADAPTER=sqlite start-server-and-test 'sintra reset-database && yarn start' http://127.0.0.1:3000 'yarn cypress:run'",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "blognami": "^0.22.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "cypress": "^12.2.0",
    "jest": "^28.0.1",
    "start-server-and-test": "^1.15.2"
  }
}
