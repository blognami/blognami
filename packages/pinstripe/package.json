{
  "type": "module",
  "name": "pinstripe",
  "description": "Pinstripe is a fullstack JavaScript web framework for Node.js and the browser that follows the model-view-controller (MVC) design pattern.",
  "version": "0.8.0",
  "author": "Jody Salt",
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js"
  },
  "bin": {
    "pinstripe": "./cli.js"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "build": "yarn build:css",
    "build:css": "sass --load-path=../../node_modules/ lib/scss/index.scss lib/static/bundle.css",
    "start": "IS_LOCAL_PINSTRIPE=true pinstripe start-server",
    "test": "yarn test:unit",
    "test:unit": "jest lib"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.1.0",
    "bulma": "^0.9.3",
    "jest": "^26.6",
    "ramda": "^0.27.0",
    "sass": "^1.36.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "busboy": "^0.3.1",
    "chalk": "^4.1.0",
    "html-entities": "^2.3.2",
    "memfs": "^3.2.2",
    "mime-types": "^2.1.28",
    "mysql": "^2.18.1",
    "qs": "^6.9.4",
    "unionfs": "^4.4.0",
    "webpack": "^5.39.1"
  }
}
